mixin sectionProgramacao()
	.row
		.title-section
			h2="Programação"
		ul.timeline
			li
				.box-talk
					time.talk-time="8:30 - 9:30"
					h4.sub-title="Credenciamento"
					p="Não esqueça de trazer seu RG."
			li
				.box-talk
					time.talk-time="9:00 - 9:30"
					h4.sub-title="Coffee Break"
					p="Antes do evento começar, aproveite para tomar um café e atualizar o seu networking."

			li
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Ciro Nunes', src='https://avatars2.githubusercontent.com/u/469908?v=3&s=400')
					|     
					h3.sub-title Ciro Nunes
					|     
					p
						| Senior Front-end Engineer at 
						a(href='http://www.questrade.com', target='_blank') Questrade
					|     
					p
						i.icon-social-twitter
						|       
						a.hover.hover-3d.speaker-link(href='https://twitter.com/cironunesdev', target='_blank')
							span(data-title='@cironunesdev') @cironunesdev
				.box-talk
					time.talk-time="9:00 - 9:30"
					h4.sub-title="Abertura"
					p="Um pouco do que vai rolar."
			li
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Willian Justen', src='assets/image/will.jpg')
					|     
					h3.sub-title Willian Justen
					|     
					p
						| Desenvolvedor Web
						a(href='http://www.globo.com', target='_blank')  Globo.com
					|     
					p
						i.icon-social-twitter
						|       
						a.hover.hover-3d.speaker-link(href='https://twitter.com/Willian_justen', target='_blank')
							span(data-title='@Willian_justen') @Willian_justen
				.box-talk
					time.talk-time="10:00 - 10:40"
					h4.sub-title="Criando novas experiências com SVG"
					p="Nessa palestra eu irei mostrar como o SVG pode melhorar e modificar as experiências do usuário em um projeto e como você já pode sair da palestra utilizando."
			li
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Jaydson Gomes', src='assets/image/jaydson.jpg')
					|     
					h3.sub-title Jaydson Gomes
					|     
					p
						| Front-End Engineer
						a(href='http://www.terra.com.br', target='_blank')  Portal Terra
					|     
					p
						i.icon-social-twitter
						|       
						a.hover.hover-3d.speaker-link(href='https://twitter.com/jaydson', target='_blank')
							span(data-title='@jaydson') @jaydson
				.box-talk
					time.talk-time="00:00 - 00:00"
					h4.sub-title="A DEFINIR"
			li
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Diego Ramos', src='assets/image/diegoramos.jpg')
					|     
					h3.sub-title Diego Ramos
					|     
					p
						| Analista de Sistemas Sênior
						a(href='http://www.bematech.com.br/', target='_blank')  Bematech
				.box-talk
					time.talk-time="00:00 - 00:00"
					h4.sub-title="A DEFINIR"
			li
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Leo Balter', src='assets/image/leobalter.jpg')
					|     
					h3.sub-title Leo Balter
					|     
					p
						| Open Web Engineer
						a(href='http://bocoup.com', target='_blank')  Bocoup
				.box-talk
					time.talk-time="00:00 - 00:00"
					h4.sub-title="A DEFINIR"
			li
				.box-talk
					time.talk-time="19:30 - 20:00"
					h4.sub-title="Encerramento"